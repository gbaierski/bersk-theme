	"tokenColors": [
		{
			"settings": {
				"foreground": "#cccccc"
			}
		},
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded",
				"string meta.image.inline.markdown"
			],
			"settings": {
				"foreground": "#F8F8F2"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#555657"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#75bde0"
			}
		},
		{
			"name": "Template Definition",
			"scope": [
				"punctuation.definition.template-expression",
